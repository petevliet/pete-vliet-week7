<%= form_for @user do |f| %>

<%= render partial: 'shared/errors', locals: {object: @user} %>

<br />
  <%= f.label :user_name %>
  <%= f.text_field :user_name %>
<br />
  <%= f.label :password %>
  <%= f.password_field :password %>
<br />
  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>
<br />
  <%= f.label :about %>
  <%= f.text_field :about %>
<br />
  <%= f.submit %>
<br />
  <%= link_to 'cancel', root_path %>

<% end %>
